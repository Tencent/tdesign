  "main": "dist/td-theme-generator.js",
  "files": [
    "dist/td-theme-generator.js"
  ],
  "keywords": [
    "tdesign",
    "theme",
    "generator"
  ],
  "contributors": [
    "uyarn",
    "mingrutough1",
    "yilaierwang"
  ],
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.8.3",
    "cssbeautify": "^0.3.1",
    "lodash": "^4.17.21",
    "tdesign-icons-vue": "^0.0.8",
    "tdesign-vue": "1.4.6-naruto",
    "tvision-color": "^1.6.0",
    "vue": "2.7.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
